<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                               http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create-constrain-address-monitoring_object_property_address_fk" author="M_Zhivotovskiy">
        <addForeignKeyConstraint
                baseTableName="address"
                baseColumnNames="subject_id"
                constraintName="monitoring_object_property_address_fk"
                referencedTableName="address_subject"
                referencedColumnNames="id"/>
    </changeSet>

    <changeSet id="create-constrain-address-monitoring_object_property_address_fk_0" author="M_Zhivotovskiy">
        <addForeignKeyConstraint
                baseTableName="address"
                baseColumnNames="building_id"
                constraintName="monitoring_object_property_address_fk_0"
                referencedTableName="address_building"
                referencedColumnNames="id"/>
    </changeSet>

    <changeSet id="create-constrain-address-monitoring_object_property_address_fk_1" author="M_Zhivotovskiy">
        <addForeignKeyConstraint
                baseTableName="address"
                baseColumnNames="settlement_id"
                constraintName="monitoring_object_property_address_fk_1"
                referencedTableName="address_settlement"
                referencedColumnNames="id"/>
    </changeSet>

    <changeSet id="create-constrain-address-monitoring_object_property_address_fk_2" author="M_Zhivotovskiy">
        <addForeignKeyConstraint
                baseTableName="address"
                baseColumnNames="town_id"
