    </changeSet>

    <changeSet id="create-constrain-address-monitoring_object_property_address_fk_6" author="M_Zhivotovskiy">
        <addForeignKeyConstraint
                baseTableName="address"
                baseColumnNames="street_id"
                constraintName="monitoring_object_property_address_fk_6"
                referencedTableName="address_street"
                referencedColumnNames="id"/>
    </changeSet>
</databaseChangeLog>