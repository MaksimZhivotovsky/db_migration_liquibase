<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                               http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create-constrain-organization_property_address-organization_property_address_fk"
               author="M_Zhivotovskiy">
        <addForeignKeyConstraint
                baseTableName="organization_property_address"
                baseColumnNames="address_subject_id"
                constraintName="organization_property_address_fk"
                referencedTableName="address_subject"
                referencedColumnNames="id"/>
    </changeSet>

