               author="M_Zhivotovskiy">
        <addForeignKeyConstraint
                baseTableName="organization_property_address"
                baseColumnNames="town_id"
                constraintName="organization_property_address_fk_4"
                referencedTableName="address_town"
                referencedColumnNames="id"/>
    </changeSet>
</databaseChangeLog>